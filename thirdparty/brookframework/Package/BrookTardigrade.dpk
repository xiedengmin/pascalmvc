package BrookTardigrade;

{$R *.res}
{$IFDEF IMPLICITBUILDING This IFDEF should not be used by users}
{$ALIGN 8}
{$ASSERTIONS ON}
{$BOOLEVAL OFF}
{$DEBUGINFO OFF}
{$EXTENDEDSYNTAX ON}
{$IMPORTEDDATA ON}
{$IOCHECKS ON}
{$LOCALSYMBOLS ON}
{$LONGSTRINGS ON}
{$OPENSTRINGS ON}
{$OPTIMIZATION OFF}
{$OVERFLOWCHECKS OFF}
{$RANGECHECKS OFF}
{$REFERENCEINFO ON}
{$SAFEDIVIDE OFF}
{$STACKFRAMES ON}
{$TYPEDADDRESS OFF}
{$VARSTRINGCHECKS ON}
{$WRITEABLECONST OFF}
{$MINENUMSIZE 1}
{$IMAGEBASE $400000}
{$DEFINE DEBUG}
{$ENDIF IMPLICITBUILDING}
{$DESCRIPTION 'Brook is microframework which helps to develop web Pascal applications.'}
{$IMPLICITBUILD ON}

requires
  rtl,
  designide;

contains
  BrookExtra in '..\Source\BrookExtra.pas',
  BrookHandledClasses in '..\Source\BrookHandledClasses.pas',
  BrookHTTPAuthentication in '..\Source\BrookHTTPAuthentication.pas',
  BrookHTTPCookies in '..\Source\BrookHTTPCookies.pas',
  BrookHTTPRequest in '..\Source\BrookHTTPRequest.pas',
  BrookHTTPResponse in '..\Source\BrookHTTPResponse.pas',
  BrookHTTPServer in '..\Source\BrookHTTPServer.pas',
  BrookHTTPUploads in '..\Source\BrookHTTPUploads.pas',
  BrookIDEIntegration in 'BrookIDEIntegration.pas',
  BrookLibraryLoader in '..\Source\BrookLibraryLoader.pas',
  BrookLogger in '..\Source\BrookLogger.pas',
  BrookMathExpression in '..\Source\BrookMathExpression.pas',
  BrookMediaTypes in '..\Source\BrookMediaTypes.pas',
  BrookReader in '..\Source\BrookReader.pas',
  BrookString in '..\Source\BrookString.pas',
  BrookStringMap in '..\Source\BrookStringMap.pas',
  BrookURLEntryPoints in '..\Source\BrookURLEntryPoints.pas',
  BrookURLRouter in '..\Source\BrookURLRouter.pas',
  BrookUtility in '..\Source\BrookUtility.pas',
  libsagui in '..\Source\libsagui.pas',
  Marshalling in '..\Source\Marshalling.pas',
  Platform in '..\Source\Platform.pas';

end.
